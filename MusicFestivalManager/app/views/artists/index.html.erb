<table><h2>Artist Performances</h2>

<tr>
  <th>Artist</th>
  <th>Stage</th>
  <th>Start Time</th>
  <th>End Time</th>
</tr>
  <% @artists.each do |artist| %>
  <tr>
    <td> <%= artist.name %> </td>
    <td> <%= artist.performance.stage.name if artist.performance %> </td>
    <td> <%= artist.performance.start_time if artist.performance %> </td>
    <td> <%= artist.performance.end_time if artist.performance %> </td>
  </tr>
  <% end %>
</table>

<%#= render "common/nav_all" %>
